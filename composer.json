{
  "name": "upstatement/timber-starter-theme",
  "description": "Starter theme to build a Timber theme",
  "type": "wordpress-theme",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "jarednova",
      "email": "jared@upstatement.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "git",
      "url": "https://github.com/mdahlke/atomic-docs.git"
    },
    {
      "type": "git",
      "url": "git@bitbucket.org:wisnet/debugalias.git"
    }
  ],
  "extra": {
    "installer-paths": {
      "../../plugins/{$name}/": [
        "wpackagist-plugin/*",
        "type:wordpress-plugin"
      ]
    }
  },
  "require": {
    "wpackagist-plugin/timber-library": "1.*",
    "nesbot/carbon": "^1.36",
    "duncan3dc/dusk": "^1.0"
  },
  "require-dev": {
  }
}
